package data.datahelper;

import java.util.Map;

import po.CreditPo;
import po.OrderPo;

public interface CreditDataHelper {
	
	/**
	 * @return	从数据库中读取信用信息
	 */
	public Map<Integer,CreditPo> getCreditData();
	
	/**
	 * 向数据库中写入信用信息
	 * @param list	
	 */
	public void addOrderData(OrderPo orderPo);

}
