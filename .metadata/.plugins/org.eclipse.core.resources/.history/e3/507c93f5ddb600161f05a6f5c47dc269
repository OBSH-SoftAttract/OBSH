package data.dao;
/**
 * @author bxh
 */

import java.util.List;

import po.CreditPo;

public interface CreditDao {
	
	/**
	 * @param orderId
	 * @return	获取客户信用信息
	 */
	public List<CreditPo> getCredit(int userId);
	
	/**
	 * @param creditPo
	 * @return  添加客户的信用信息
	 */
	public boolean setCredit(CreditPo creditPo);
	
}

